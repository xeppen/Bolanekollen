<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ScrollView02"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/interestDialogHeadlineRow"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        android:padding="@dimen/general_padding" >

        <TextView
            android:id="@+id/interestDialogHeadlineTextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:text="@string/interestDialogHeadlineTextView" >
        </TextView>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/openURL"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        android:padding="@dimen/general_padding" >

        <TextView
            android:id="@+id/openURLTextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:text="@string/interestDialog1ContentTextView"
            android:textSize="@dimen/textSize" >
        </TextView>
    </LinearLayout>

    

</LinearLayout>